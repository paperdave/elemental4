{
  "$schema": "../../../res/theme.schema.json",
  "type": "elemental4:theme",
  "format_version": 1,
  "version": 1,
  "id": "elem4_default",
  "name": "Elemental 4",
  "author": "Dave Caruso",
  "description": "The default look of Elemental 4.",
  "styles": "./style.css",
  "icon": "./icon.png",
  "sound_merge_mode": "override",
  "sounds": {
    "element.combine": [{ "url": "sound/element-combine.mp3" }],
    "element.invalid": [{ "url": "sound/element-invalid.mp3" }],
    "element.pickup": [{ "url": "sound/element-pickup.mp3" }],
    "element.valid": [{ "url": "sound/element-valid.mp3" }],
    "element.valid.rediscover": [{ "url": "sound/element-valid.mp3" }],
    "element.drop": [{ "url": "sound/element-drop.mp3" }],
    "news.new-element": [],
    "dialog.open": [{ "url": "sound/popup-open.mp3", "volume": 0.8 }],
    "dialog.close": [{ "url": "sound/popup-close.mp3", "volume": 0.8 }],
    "dialog.button": [{ "url": "sound/popup-click.mp3", "volume": 0.65 }],
    "suggest.created": [],
    "suggest.voted": [],
    "startup": [{ "url": "sound/startup.mp3", "volume": 0.4 }]
  },
  "music_merge_mode": "combine",
  "music": [
    {
      "fade": "fade-both",
      "loop": "loop",
      "url": "music/elemental4.mp3",
      "title": "Elemental 4 Song",
      "author": "Justin Tan"
    }
  ],
  "colors": {
    "white": {
      "color": "#FFFFFF",
      "saturationMultiplier": 0,
      "lightnessMultiplier": 0
    },
    "black": {
      "color": "#000000",
      "saturationMultiplier": 0,
      "lightnessMultiplier": 0
    },
    "grey": {
      "color": "#7F7F7F",
      "saturationMultiplier": 0,
      "lightnessMultiplier": 1.95
    },
    "brown": {
      "color": "#B58148",
      "saturationMultiplier": 0.5,
      "lightnessMultiplier": 1
    },
    "red": {
      "color": "#E35454",
      "saturationMultiplier": 1,
      "lightnessMultiplier": 1
    },
    "orange": {
      "color": "#E38454",
      "saturationMultiplier": 1,
      "lightnessMultiplier": 1
    },
    "yellow": {
      "color": "#E3CB54",
      "saturationMultiplier": 1,
      "lightnessMultiplier": 1
    },
    "yellow-green": {
      "color": "#B3E354",
      "saturationMultiplier": 1,
      "lightnessMultiplier": 1
    },
    "green": {
      "color": "#54E360",
      "saturationMultiplier": 1,
      "lightnessMultiplier": 1
    },
    "aqua": {
      "color": "#54E3B3",
      "saturationMultiplier": 1,
      "lightnessMultiplier": 1
    },
    "blue": {
      "color": "#54b3e3",
      "saturationMultiplier": 1,
      "lightnessMultiplier": 1
    },
    "dark-blue": {
      "color": "#5475e3",
      "saturationMultiplier": 1,
      "lightnessMultiplier": 1
    },
    "purple": {
      "color": "#8654E3",
      "saturationMultiplier": 1,
      "lightnessMultiplier": 1
    },
    "magenta": {
      "color": "#D054E3",
      "saturationMultiplier": 1,
      "lightnessMultiplier": 1
    },
    "hot-pink": {
      "color": "#E354A2",
      "saturationMultiplier": 1,
      "lightnessMultiplier": 1
    }
  }
}
